    "@types/node": "^11.13.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "expect.js": "^0.3.1",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "source-map-support": "^0.5.9",
